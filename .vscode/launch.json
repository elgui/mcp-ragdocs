{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/build/index.js",
            "outFiles": [
                "${workspaceFolder}/build/**/*.js"
            ],
            "envFile": "${workspaceFolder}/.env"
        },
        {
            "type": "chrome",
            "request": "launch",
            "name": "Launch Client",
            "url": "http://localhost:3030",
            "webRoot": "${workspaceFolder}/src/public"
        }
    ]
}
