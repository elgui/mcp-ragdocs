{
  "path": "D:\\Dev\\mcp-ragdocs",
  "name": "mcp-ragdocs",
  "include": [
    "**/*.md",
    "**/*.ts",
    "**/*.js",
    "**/*.html"
  ],
  "exclude": [
    "**/node_modules/**",
    "**/.git/**",
    "**/*_generated.*",
    "**/__pycache__/",
    "**/myenv/",
    "**/.git/",
    "**/backend/docs/build/",
    "**/backend/apps/main/tests/test_data/",
    "**/frontend/**",
    "**/TASK_OLD.md",
    "**/.langgraph_api/",
    "**/.VSCodeCounter/",
    "**/backend/staticfiles/",
    "**/backend/coverage.lcov",
    "**/.coverage",
    "**/backend/benchmark_results/",
    "**/backend/apps/main/migrations/",
    "**/backend/apps/user/migrations/",
    "**/backend/apps/activity/migrations/",
    "**/backend/scripts/codebase.json",
    "**/backend/test-results/junit.xml",
    "**/backend/backend/error_report.txt",
    "**/backend/test-db.sqlite3",
    "**/frontend_mock/",
    "**/frontend_enhanced/node_modules/",
    "**/frontend_enhanced/dist/",
    "**/frontend_enhanced/.env*",
    "**/codebase_export.json"
  ],
  "watchMode": true,
  "watchInterval": 60000,
  "chunkSize": 1000,
  "fileTypeConfig": {
    ".py": {
      "include": true,
      "chunkStrategy": "semantic"
    },
    ".md": {
      "include": true,
      "chunkStrategy": "semantic"
    }
  }
}